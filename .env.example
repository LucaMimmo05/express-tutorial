# Configurazione Server
PORT=3000
NODE_ENV=development

# Database MongoDB
# Per MongoDB locale: mongodb://localhost:27017/express-app
# Per MongoDB Atlas: mongodb+srv://username:password@cluster.mongodb.net/database
MONGODB_URI=mongodb://localhost:27017/express-app

# Cookie Settings
COOKIE_MAX_AGE=360000

# File Upload Settings (in bytes)
FILE_SIZE_LIMIT=1000000
